<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:gravity="center_vertical"
	android:focusable="false">
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<LinearLayout android:layout_width="1dp"
			android:layout_height="fill_parent" android:id="@+id/feed_view_row_newbar">
		</LinearLayout>
		<TextView android:id="@+id/feed_view_row_title"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center_vertical" android:textColor="#fff"
			android:paddingLeft="5dp" android:minLines="2" android:paddingTop="6dp"
			android:paddingBottom="6dp" android:textSize="15dp" />

	</LinearLayout>
	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:id="@+id/feed_view_row_description">
		<Button android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:visibility="gone" android:id="@+id/feed_view_row_description_enclosure"
		android:layout_margin="0px" android:focusable="false"
		android:maxLines="2"/>
		<TextView android:id="@+id/feed_view_row_description_text"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center_vertical" android:textColor="#ddd"
			android:paddingLeft="5dp"
			android:background="#ff3f3f3f" android:minLines="1" />
	</LinearLayout>
</LinearLayout>